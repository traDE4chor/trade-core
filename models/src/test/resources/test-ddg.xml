<ddg:DataDependenceGraph xmlns:ddg="http://de.uni-stuttgart/iaas/trade/ddg">
    <processors>
        <processor identifier="act_A" name="ActivityA">
            <incoming>
                <edge>readInput</edge>
            </incoming>
            <outgoing>
                <edge>setResult</edge>
                <edge>setFlag</edge>
            </outgoing>
            <type>ACTIVITY</type>
        </processor>

        <processor identifier="partic_B" name="ParticipantB">
            <incoming>
                <edge>readResult</edge>
            </incoming>
            <type>PARTICIPANT</type>
        </processor>
    </processors>

    <dataObjects>
        <dataObject identifier="do_input" name="input">
            <outgoing>
                <edge>readInput</edge>
            </outgoing>
            <type>DATA</type>
            <dataElements>
                <dataElement identifier="de_partA" name="partA">
                    <typeDefinition>string</typeDefinition>
                </dataElement>
                <dataElement identifier="de_partB" name="partB">
                    <typeDefinition>boolean</typeDefinition>
                </dataElement>
            </dataElements>
        </dataObject>

        <dataObject identifier="do_output" name="output">
            <type>DATA</type>
            <dataElements>
                <dataElement identifier="de_result" name="string">
                    <incoming>
                        <edge>setResult</edge>
                    </incoming>
                    <outgoing>
                        <edge>readResult</edge>
                    </outgoing>
                    <typeDefinition>file</typeDefinition>
                </dataElement>
                <dataElement identifier="de_flag" name="string">
                    <incoming>
                        <edge>setFlag</edge>
                    </incoming>
                    <typeDefinition>boolean</typeDefinition>
                </dataElement>
            </dataElements>
        </dataObject>
    </dataObjects>

    <dependenceEdges>
        <dependenceEdge identifier="readInput" name="readInput">
            <source>do_input</source>
            <sourceSelector>de_partA</sourceSelector>
            <target>act_A</target>
            <targetSelector>/process/variables/variable[1]</targetSelector>
        </dependenceEdge>
        <dependenceEdge identifier="setResult" name="setResult">
            <source>act_A</source>
            <sourceSelector>/process/variables/variable[2]</sourceSelector>
            <target>de_result</target>
        </dependenceEdge>
        <dependenceEdge identifier="setFlag" name="setFlag">
            <source>act_A</source>
            <sourceSelector>/process/variables/variable[3]</sourceSelector>
            <target>de_flag</target>
        </dependenceEdge>
        <dependenceEdge identifier="readResult" name="readResult">
            <source>de_result</source>
            <target>partic_B</target>
            <targetSelector>/process/variables/variable[4]</targetSelector>
        </dependenceEdge>
    </dependenceEdges>
</ddg:DataDependenceGraph>